{
    "name": "skiddph/api",
    "description": "An out of the box complete and centralized general purpose API system that aims to power you web development journey.",
    "require": {
        "vlucas/phpdotenv": "^5.5",
        "robmorgan/phinx": "^0.13.3",
        "phpmailer/phpmailer": "^6.7",
        "aws/aws-sdk-php": "^3.255"
    },
    "autoload": {
        "psr-4": {
            "SkiddPH\\": "system/",
            "eru123\\": "system/eru123/"
        },
        "files": [
            "system/functions.php"
        ]
    },
    "scripts": {
        "pre-autoload-dump": "Aws\\Script\\Composer\\Composer::removeUnusedServices"
    },
    "extra": {
        "aws/aws-sdk-php": [
            "S3"
        ]
    },
    "license": "Apache-2.0"
}
